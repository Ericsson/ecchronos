{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "List of sNodes",
  "description": "A list of Nodes held within ecchronos",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "ecchronosId": {
        "type": "string"
      },
      "datacenterName": {
        "type": "string"
      },
      "nodeID": {
        "type": "string"
      },
      "lastConnection": {
        "type": "string"
      },
      "nextConnection": {
        "type": "string"
      },
      "nodeEndpoint": {
        "type": "string"
      },
      "nodeStatus": {
        "type": "string"
      }
    }
  }
}